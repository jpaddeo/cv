---
import MercadoLibre from '@/components/sections/visitors/MercadoLibre.astro';
import Interbanking from '@/components/sections/visitors/Interbanking.astro';
import Default from '@/components/sections/visitors/Default.astro';

export const prerender = false;

const visitor = Astro.cookies.get('v');

const VisitorComponents = {
  meli: MercadoLibre,
  interbanking: Interbanking,
  default: Default,
};

let Component = Fragment;
if (visitor && visitor.value.length > 0) {
  Component =
    VisitorComponents[visitor.value as keyof typeof VisitorComponents] ||
    Default;
}
---

<Component />
